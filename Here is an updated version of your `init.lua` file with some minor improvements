-- ...

-- Set up leader key
vim.api.nvim_set_keymap('n', '<Leader>', '', { noremap = true, silent = true })

-- Map common keys
vim.api.nvim_set_keymap('n', '<C-f>', ':nnoremap <silent> <C-f> :call OnFocus() | endif', { noremap = true })
vim.api.nvim_set_keymap('n', '<C-F>', ':nnoremap <silent> <C-F> :call OnFocus() | endif', { noremap = true })

-- ...
