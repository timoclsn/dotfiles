{
  "use client": {
    "prefix": "uc",
    "body": ["'use client';"]
  },
  "Function Component": {
    "prefix": "fc",
    "body": ["export const ${TM_FILENAME_BASE} = () => {", "  return $1;", "};"]
  },
  "Function Component with Props": {
    "prefix": "fcp",
    "body": [
      "interface Props {",
      "  $1",
      "}",
      "",
      "export const ${TM_FILENAME_BASE} = ({$2}: Props) => {",
      "  return $3;",
      "};"
    ]
  },
  "React Native Style": {
    "prefix": "rns",
    "body": ["style={styles.$1}"]
  }
}
